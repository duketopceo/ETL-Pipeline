# Google Cloud Configuration Template
# Copy this file to .env and fill in your values

# Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-project-id-here

# Path to service account key file (optional - only if using service account authentication)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# BigQuery Configuration
BIGQUERY_DATASET=etl_pipeline_data
BIGQUERY_TABLE=master_job_data

# Authentication Method (choose one):
# 1. service_account - Use service account key file
# 2. gcloud - Use gcloud CLI authentication  
# 3. default - Use application default credentials
AUTH_METHOD=gcloud